{
  "name": "jsoncrack-vscode",
  "displayName": "JSON Crack",
  "license": "MIT",
  "description": "Visualize JSON data into graphs",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Visualization",
    "Data Science"
  ],
  "activationEvents": [
    "onCommand:jsoncrack-vscode.start",
    "onLanguage:json"
  ],
  "main": "./build/ext-src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "jsoncrack-vscode.start",
        "title": "View Graph",
        "category": "menubar",
        "icon": {
          "light": "./assets/icon.png",
          "dark": "./assets/icon.png"
        }
      }
    ],
    "menus": {
      "editor/title": [
        {
          "command": "jsoncrack-vscode.start",
          "when": "editorLangId == json",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "yarn run package",
    "start": "react-scripts start",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "yarn run compile-tests && yarn run compile && yarn run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js",
    "build": "./scripts/build-non-split.js && tsc -p tsconfig.extension.json",
    "watch-build": "nodemon --watch src --ext js,tsx,ts --exec \"yarn run build\"",
    "eject": "react-scripts eject"
  },
  "devDependencies": {
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.1",
    "@types/node": "16.x",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/styled-components": "^5.1.26",
    "@types/vscode": "^1.71.0",
    "@typescript-eslint/eslint-plugin": "^5.31.0",
    "@typescript-eslint/parser": "^5.31.0",
    "@vscode/test-electron": "^2.1.5",
    "eslint": "^8.20.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.20",
    "react-scripts": "^5.0.1",
    "rewire": "^6.0.0",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.4.0",
    "react-linkify-it": "^1.0.7",
    "react-zoom-pan-pinch": "^2.1.3",
    "reaflow": "^5.0.6",
    "styled-components": "^5.3.5",
    "terser": "^5.15.0",
    "zustand": "^4.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
